{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "jest": true
  },
  "plugins": ["import", "jest"],
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-unused-expressions": "off",
    "max-len": "off",
    "semi": ["error", "never"],
    "no-console": ["error", { "allow": ["log", "error", "warn"] }],
    "no-underscore-dangle": ["off"],
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "parent", "sibling", "index"],
        "newlines-between": "always"
      }
    ]
  }
}
